{
  "manifest_version": 3,
  "name": "tempext",
  "version": "1.0",
  "description": "ChatGPT background color switcher",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}