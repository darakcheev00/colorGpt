{
    "manifest_version": 2,
    "name": "Color Changer Extension",
    "version": "1.0",
    "description": "Change background color of a specific div",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "logo.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  